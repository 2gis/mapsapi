<div class="dg-popup-header-title dg-popup-header-title__firmcard" title="{firmName}">{firmName}</div>

{?links}
    <div class="popup_header_links">
        {#links}
            {@eq key="{name}" value="flamp_stars" type="string"}
                <div class="popup-link popup-link_type_{name}">
                    <div{#width} style="width: {.}%;"{/width}></div>
                </div>
            {:else}
                <a class="popup-header-link popup-link popup-link_type_{name}"
                    {?href}
                        href={href} target="_blank"
                    {:else}
                        href="javascript:void(0)"
                    {/href}
                    >{label}</a>
            {/eq}
        {/links}
    </div>
{/links}
